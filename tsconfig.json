{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": "./",
    "paths": {
      "@root/*": ["./*"],
      "@app/*": ["app/*"],
      "@model/*": ["app/model/*"],
      "@presentation/*": ["app/presentation/*"],
      "@repository/*": ["app/repository/*"],
      "@utils/*": ["app/utils/*"]
    }
  }
}
